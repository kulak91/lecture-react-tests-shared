{
  "name": "tests",
  "private": "true",
  "engines": {
    "node": ">=14 <17",
    "npm": ">=6 <9"
  },
  "scripts": {
    "test:base": "wdio run ./wdio.conf.ts --suite base",
    "test:advanced": "wdio run ./wdio.conf.ts --suite advanced",
    "lint:js": "npx eslint \"src/**/*.ts\"",
